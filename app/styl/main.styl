			 url('../fonts/OpenSansRegular/opensansregular-webfont.woff2') format('woff2'), /* Super Modern Browsers */
			 url('../fonts/OpenSansRegular/opensansregular-webfont.woff') format('woff'), /* Pretty Modern Browsers */
			 url('../fonts/OpenSansRegular/opensansregular-webfont.ttf') format('truetype'), /* Safari, Android, iOS */
			 url('../fonts/OpenSansRegular/opensansregular-webfont.svg#svgFontName') format('svg')
